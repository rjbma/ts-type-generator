{
  "name": "ts-type-generator",
  "engines": {
    "node": ">10"
  },
  "version": "0.1.8",
  "description": "Generate typescript types for an OpenAPI v3 spec",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjbma/ts-type-generator"
  },
  "bin": {
    "generate-ts-types": "./bin/global.js"
  },
  "files": [
    "/build"
  ],
  "preferGlobal": true,
  "scripts": {
    "start": "ts-node src/generate-types.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.37",
    "@types/prettier": "^2.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "dtsgenerator": "3.7.1",
    "dtsgenerator-express-route-types": "^0.1.6",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "tempy": "^1.0.1"
  }
}
