{
  "name": "ts-type-generator",
  "engines": {
    "node": ">10"
  },
  "version": "0.3.4",
  "description": "Generate typescript types for an OpenAPI v3 spec",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjbma/ts-type-generator"
  },
  "bin": {
    "generate-ts-types": "./bin/global.js"
  },
  "files": [
    "/build"
  ],
  "preferGlobal": true,
  "scripts": {
    "start_old": "ts-node src/generate-types.ts",
    "start": "node --inspect -r ts-node/register src/generate-types.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.12",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "@dtsgenerator/replace-namespace": "^1.5.4",
    "dtsgenerator": "^3.18.0",
    "dtsgenerator-express-route-types": "^0.2.8",
    "glob": "^10.2.7",
    "json-schema": "^0.4.0",
    "json-schema-ref-parser": "^9.0.9",
    "minimist": "^1.2.8",
    "prettier": "^2.8.8",
    "tempy": "^3.0.0"
  }
}
