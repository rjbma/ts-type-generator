{
  "name": "ts-type-generator",
  "engines": {
    "node": ">10"
  },
  "version": "0.3.0",
  "description": "Generate typescript types for an OpenAPI v3 spec",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjbma/ts-type-generator"
  },
  "bin": {
    "generate-ts-types": "./bin/global.js"
  },
  "files": [
    "/build"
  ],
  "preferGlobal": true,
  "scripts": {
    "start": "ts-node src/generate-types.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.31",
    "@types/prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@dtsgenerator/replace-namespace": "^1.5.2",
    "dtsgenerator": "^3.15.1",
    "dtsgenerator-express-route-types": "^0.2.5",
    "glob": "^8.0.1",
    "json-schema": "^0.4.0",
    "json-schema-ref-parser": "^9.0.9",
    "minimist": "^1.2.6",
    "prettier": "^2.6.2",
    "tempy": "^3.0.0"
  }
}
